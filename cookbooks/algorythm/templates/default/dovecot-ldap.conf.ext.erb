hosts = <%=@host%>
dn = cn=<%=@user%>
dnpass = <%=@password%>
tls = no
auth_bind = yes
auth_bind_userdn = cn=%u,ou=People,<%=@suffix%>
base = ou=People,<%=@suffix%>
user_attrs = 
user_filter = (&(objectClass=mailRecipient)(cn=%u))
pass_attrs = uid=cn,userPassword=userPassword
default_pass_scheme = SSHA
scope = subtree
ldap_version = 3

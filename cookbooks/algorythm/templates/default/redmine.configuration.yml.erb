production:
  attachments_storage_path: <%=@homeDir%>/files
  email_delivery:
    delivery_method: :smtp
    smtp_settings:
      address: "<%=@mailServerHost%>"
      port: 25
      domain: "<%=@mailServerHost%>"
      authentication: :plain
      user_name: "<%=@mailServerUser%>"
      password: "<%=@mailServerPassword%>" 
      tls: false
      enable_starttls_auto: false
